{
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": false
    },
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^build"],
      "outputs": [
        "build/**",
        ".vercel/**",
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ],
      "env": [
        "CLERK_PUBLISHABLE_KEY",
        "CLERK_SECRET_KEY",
        "CLERK_SIGN_IN_URL",
        "CLERK_SIGN_UP_URL",
        "CLERK_SIGN_IN_FALLBACK_URL",
        "CLERK_SIGN_UP_FALLBACK_URL"
      ]
    },
    "client#build": {
      "dependsOn": ["^build"],
      "env": [
        "CLERK_PUBLISHABLE_KEY",
        "CLERK_SECRET_KEY",
        "CLERK_SIGN_IN_URL",
        "CLERK_SIGN_UP_URL",
        "CLERK_SIGN_IN_FALLBACK_URL",
        "CLERK_SIGN_UP_FALLBACK_URL"
      ],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "check-types": {
      "dependsOn": ["^build", "^check-types"]
    }
  }
}
